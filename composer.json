{
    "name": "rampin97/simpnas",
    "description": "Simple Network Attached Storage System for your home or small business",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Simpnas\\": "src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "php-di/php-di": "^6.3",
        "phpmailer/phpmailer": "^6.5",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "slim/twig-view": "^3.0",
        "twig/twig": "^3.0"
    },
    "config": {
        "process-timeout" : 0,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    }
}
